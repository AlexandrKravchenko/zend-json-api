{
    "name": "vallarj/zend-jsonapi",
    "description": "JSONAPI implementation for Zend Framework 3",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Vallarj\\JsonApi\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "Vallarj\\JsonApi"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
