{
    "name": "vallarj/zend-json-api",
    "description": "JSON API library for Zend Framework 3",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "doctrine/inflector": "^1.2",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-validator": "^2.10",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Vallarj\\JsonApi\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "Vallarj\\JsonApi"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
